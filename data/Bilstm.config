[PATH]
# Paths To Datasets And Evaluation
path_train : ../data/train.txt
path_dev : ../data/dev.txt
path_test : ../data/test.txt
path_stopwords: ../data/stopwords.txt
# Using pre-trained Embeddings
path_pre_emb : ../data/glove.txt
path_eval_result : ../data/output.txt


[Model]
# Options for model
model : bilstm
path_model : ../data/model.bilstm

# Model Settings
epoch : 10
lowercase : false

# Network Structure
embedding_dim : 200
batch_size : 20

# Hyperparameters
lr_param : 0.01
momentum=0.9


[Ensemble]
# Ensemble model
model : bilstm_ensemble
ensemble_size : 5
path_model : ../data/model.bilstm_ensemble